    <JFXDatePicker prefWidth="180" promptText="Ngày sinh" GridPane.columnIndex="1" GridPane.rowIndex="2" />

    <Label alignment="CENTER_RIGHT" text="Vai trò:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <JFXComboBox prefWidth="180" promptText="Vai trò" GridPane.columnIndex="1" GridPane.rowIndex="3" />
