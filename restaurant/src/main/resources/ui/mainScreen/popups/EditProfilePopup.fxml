    <Label alignment="CENTER_RIGHT" text="Tên đầy đủ:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <JFXTextField prefWidth="180" promptText="Tên đầy đủ" GridPane.columnIndex="1" GridPane.rowIndex="1" />

